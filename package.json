{
  "name": "la-croix-quiz",
  "author": "Daniel Falkov",
  "description": "Logiciel de quiz scolaire proposé par Saint Jean & La Croix, permettant de lancer un quiz joignable sur un réseau local par les élèves depuis n'importe quel navigateur web.",
  "private": true,
  "version": "0.0.4",
  "scripts": {
    "dev": "vite",
    "build": "vite build && electron-builder",
    "build-win": "vite build && electron-builder build -w --ia32 --x64",
    "pbuild": "electron-builder",
    "pbuild-win": "electron-builder build -w --ia32 --x64",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-session": "^1.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.71.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "@vitejs/plugin-react": "^4.0.4",
    "electron": "^26.1.0",
    "electron-builder": "^24.6.4",
    "eslint": "^8.48.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-electron": "^0.14.0",
    "vite-plugin-electron-renderer": "^0.14.5"
  },
  "main": "dist-electron/main.js"
}
